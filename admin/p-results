    <script type="text/javascript">
      var lastOpened=false;
      var openedCollapsed=false;
      $('div[id^="list"]').on('show.bs.collapse', function () {
         if(lastOpened!=false){
            $('#'+lastOpened).collapse('hide');
            var id=$('.mobile-student-record li.'+lastOpened).removeClass('active-list');
            $('.mobile-student-record li.'+lastOpened+' a').removeClass('active');
            $('.mobile-student-record li.'+lastOpened+' i').removeClass('fa-caret-right');
            $('.mobile-student-record li.'+lastOpened+' i').addClass('fa-caret-down');

         }

         lastOpened = $(this).attr('id');
         $('.mobile-student-record li.'+lastOpened).addClass('active-list');
         $('.mobile-student-record li.'+lastOpened+' a').addClass('active');
          $('.mobile-student-record li.'+lastOpened+' i').addClass('fa-caret-right');
         list= lastOpened.substr(4);
        
      })
    </script>
